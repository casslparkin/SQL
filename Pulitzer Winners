SELECT*FROM Author_Info;

Create TABLE Work_bio 
(id INTEGER PRIMARY KEY,
Author_Id INTEGER, 
Debut_Novel TEXT, 
DN_Pub_Year INTEGER, 
Pulitzer_Winner TEXT, 
Pulitzer_Year INTEGER);

